{
  "name": "price-comparison-app",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "client:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "client": "cross-env NODE_ENV=production webpack -p",
    "server:dev": "cross-env NODE_ENV=development nodemon --watch server --ignore server/crawler/**/* --exec babel-node server/index.js",
    "server": "cross-env NODE_ENV=production webpack --config ./webpack.node.config.js -p",
    "dev": "concurrently --kill-others-on-fail \"npm run server:dev\" \" npm run client:dev\"",
    "crawler:dev": "cross-env NODE_ENV=development babel-node server/crawler/index.js",
    "heroku-postbuild": "concurrently \"npm run server\" \"npm run client\"",
    "crawler": "node build/crawler.js",
    "start": "node build/server.js"
  },
  "keywords": [],
  "author": "Cagdas ISIK",
  "license": "ISC",
