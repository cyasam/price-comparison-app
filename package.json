{
  "name": "price-comparison-app",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "client": "npm run start --prefix client",
    "server": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \" npm run client\"",
    "crawler:dev": "cross-env NODE_ENV=development babel-node server/crawler/index.js",
    "postinstall": "npm install --prefix client",
    "heroku-postbuild": "concurrently \"cross-env NODE_ENV=production webpack -p\" \"npm run build --prefix client\"",
    "crawler": "node build/crawler.js",
    "start": "node build/server.js"
  },
  "keywords": [],
  "author": "Cagdas ISIK",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server": "^2.9.13",
    "bcrypt": "^3.0.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "jsdom": "^15.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.14",
    "request": "^2.88.0"
  }
}
