    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
